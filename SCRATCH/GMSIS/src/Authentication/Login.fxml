   <children>
      <Label fx:id="titleID" alignment="CENTER" layoutX="178.0" layoutY="33.0" prefHeight="75.0" prefWidth="245.0" text="Welcome to GMSIS" textFill="#0768eb">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="189.0" layoutY="132.0" prefHeight="33.0" prefWidth="223.0" promptText="User ID..." />
      <PasswordField fx:id="pass" layoutX="189.0" layoutY="188.0" prefHeight="33.0" prefWidth="223.0" promptText="Password..." />
      <Button fx:id="loginBtn" layoutX="263.0" layoutY="254.0" mnemonicParsing="false" onAction="#loginButton" prefHeight="33.0" prefWidth="75.0" text="Login" />
   </children>
</AnchorPane>
