<AnchorPane id="AnchorPane" fx:id="pane" prefHeight="511.0" prefWidth="742.0" style="-fx-background-color: #85C1E9;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Authentication.AdminController">
   <children>
      <Label layoutX="305.0" layoutY="30.0" prefHeight="36.0" prefWidth="133.0" text="User Records">
