          <TableColumn fx:id="passCol" prefWidth="146.0" text="Password" />
          <TableColumn fx:id="IDCol" prefWidth="57.0" text="ID" />
            <TableColumn fx:id="firstnameCol" prefWidth="115.0" text="FirstName" />
            <TableColumn fx:id="surnameCol" prefWidth="97.0" text="Surname" />
            <TableColumn fx:id="adminCol" prefWidth="82.0" text="Admin" />
        </columns>
      </TableView>
      <TextField fx:id="firstName" layoutX="47.0" layoutY="95.0" promptText="First Name..." />
      <TextField fx:id="surname" layoutX="47.0" layoutY="136.0" promptText="Surname..." />
      <TextField fx:id="newPass" layoutX="47.0" layoutY="177.0" promptText="New Password..." />
      <Button fx:id="update" layoutX="420.0" layoutY="427.0" mnemonicParsing="false" onAction="#updatePass" prefHeight="25.0" prefWidth="119.0" text="Update Details" />
      <TextField fx:id="oldPass" editable="false" layoutX="240.0" layoutY="390.0" promptText="Old password" />
      <TextField fx:id="id" editable="false" layoutX="240.0" layoutY="347.0" promptText="ID" />

      <TextField fx:id="Pass" layoutX="240.0" layoutY="433.0" promptText="New password" />
 
      <CheckBox fx:id="admin" layoutX="71.0" layoutY="229.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="101.0" text="Select if Admin" />
      <MenuBar fx:id="menu" layoutY="1.0" prefHeight="25.0" prefWidth="742.0">
         <menus>
            <Menu fx:id="users" mnemonicParsing="false" text="Users">
               <items>
                  <MenuItem fx:id="user" mnemonicParsing="false" onAction="#users" text="User Records" />
               </items>
            </Menu>
            <Menu fx:id="customerAcc" mnemonicParsing="false" text="Customer Account">
               <items>
                  <MenuItem fx:id="customers" mnemonicParsing="false" onAction="#cus" text="Customers" />
               </items>
            </Menu>
            <Menu fx:id="vecRecord" mnemonicParsing="false" text="Vehicle Record">
               <items>
                  <MenuItem fx:id="records" mnemonicParsing="false" onAction="#vehicleRecord" text="Vehicle Records" />
                  <MenuItem fx:id="vecEntry" mnemonicParsing="false" onAction="#vehicleEntry" text="Vehicle Entry" />
