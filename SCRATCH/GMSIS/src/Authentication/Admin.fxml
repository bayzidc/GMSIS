            </Menu>
            <Menu fx:id="partsRecord" mnemonicParsing="false" text="Parts Record">
               <items>
                  <MenuItem fx:id="partsUsed" mnemonicParsing="false" onAction="#pUsed" text="Parts Used" />
                  <MenuItem fx:id="partsStock" mnemonicParsing="false" onAction="#pStock" text="Parts Stock" />
               </items>
            </Menu>
            <Menu fx:id="log" mnemonicParsing="false" text="Logout">
               <items>
                  <MenuItem fx:id="home" mnemonicParsing="false" onAction="#logout" text="Logout" />
               </items>
            </Menu>
         </menus>
      </MenuBar>

   </children>
</AnchorPane>
