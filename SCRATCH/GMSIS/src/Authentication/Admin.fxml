            </Menu>
            <Menu fx:id="diagAndRep" mnemonicParsing="false" text="Diagnosis &amp; Repair Booking">
               <items>
                  <MenuItem fx:id="booking" mnemonicParsing="false" onAction="#diag" text="Bookings" />
